{
  "dashboard": {
    "id": null,
    "title": "System Metrics",
    "panels": [
      {
        "id": 1,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job='frontend'}[5m])",
            "legendFormat": "Frontend"
          },
          {
            "expr": "rate(process_cpu_seconds_total{job='backend'}[5m])",
            "legendFormat": "Backend"
          }
        ]
      },
      {
        "id": 2,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job='frontend'}",
            "legendFormat": "Frontend"
          },
          {
            "expr": "process_resident_memory_bytes{job='backend'}",
            "legendFormat": "Backend"
          }
        ]
      }
    ]
  }
}